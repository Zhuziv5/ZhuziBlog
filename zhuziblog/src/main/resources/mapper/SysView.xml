<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.blog.mapper.SysViewMapper">
	
	<select id="listAllSysView" resultType="SysView">
		select * from sys_view
	</select>
	
	<insert id="addView" parameterType="SysView">
		insert into sys_view (ip) values (#{ip})
	</insert>


</mapper>